{
  "name": "react-node-starter",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev --workspace=web\" \"npm:dev --workspace=api\"",
    "build": "npm run build --workspace=web",
    "start": "npm start --workspace=web",
    "lint": "npm run lint --workspace=web"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
