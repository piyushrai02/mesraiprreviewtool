{
  "name": "react-node-starter",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev --workspace=web\" \"npm:dev --workspace=api\"",
    "build": "npm run build --workspace=web",
    "start": "npm start --workspace=web",
    "lint": "npm run lint --workspace=web"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "concurrently": "^9.2.1",
    "lucide-react": "^0.542.0",
    "next": "14.2.5",
    "next-themes": "^0.4.6",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
